{% import "data/navigation.twig" as navigation %}

<header class="header">

        <div class="header__logo">
            {{ logo.logo() }}
        </div>
        <nav class="header__nav">
            <div class="container">
                <ul class="header__list">
                    {% for item in navigation.navigation %}
                        <li class="header__item">
                            <a class="header__link" href="{{ item.link }}">
                                {{ item.title }}
                            </a>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </nav>
</header>
